	"version": "2.0.71",
	"description": "",
	"type": "module",
	"main": "./index.js",
	"module": "./index.js",
	"scripts": {
		"dev": "tsc-watch -b ./tsconfig.json --onSuccess \"npm run postbuild\" & pnpm dev:server",
		"dev:server": "cd /Users/bebejane/Projects/next-dato-boiler && pnpm dev",
		"clear:server": "rm -rf /Users/bebejane/Projects/next-dato-boiler/.next/.cache",
		"build": "node ./clean.js && npm run tsc && npm run postbuild",
		"postbuild": "copyfiles -u 1 src/**/*.css ./",
		"push": "npm run build && git add . && git commit -m \"update\" && git push",
		"release": "npm run push && npm version patch --force && git push && git push origin $(git describe --tags --abbrev=0) && git tag -f latest && git push origin latest --force && gh release create $(git describe --tags --abbrev=0) --generate-notes",
		"dev:cli": "tsx src/cli/index.ts",
		"tsc": "tsc -p tsconfig.json"
	},
	"exports": {
