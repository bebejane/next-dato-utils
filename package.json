{
	"name": "next-dato-utils",
	"version": "1.0.698",
	"description": "",
	"type": "module",
	"scripts": {
		"build": "node ./clean.js && npm run tsc && npm run copy-files",
		"build:micro": "bash build.sh && npm run copy-files",
		"pub": "npm version patch --force && npm run build && git add . && git commit -m \"updates\" && git push",
		"dev": "npx nodemon --watch \"src/**/*\" --watch \"./package.json\" -e tsx,ts,scss,json --exec \"npm run build\"",
		"dev:cli": "tsx src/cli/index.ts",
		"clean": "rm -rf ./dist",
		"test": "echo \"Error: no test specified\" && exit 1",
		"tsc": "tsc -p tsconfig.json",
		"copy-files": "copyfiles -u 1 src/**/*.scss ./build && rsync -a ./build/* ./",
		"rename:cjs": "renamer --find js --replace cjs ./dist/cjs/**/*",
		"rename:mjs": "renamer --find js --replace mjs .//**/*"
	},
	"main": "./index.js",
	"module": "./index.js",
	"exports": {
		"./package.json": "./package.json",
		"./utils": {
			"types": "./utils/index.d.ts",
			"import": "./utils/index.js"
		},
		"./config": {
			"types": "./config/index.d.ts",
			"import": "./config/index.js"
		},
		"./components": {
			"types": "./components/index.d.ts",
			"import": "./components/index.js"
		},
		"./hooks": {
			"types": "./hooks/index.d.ts",
			"import": "./hooks/index.js"
		},
		"./api": {
			"types": "./api/index.d.ts",
			"import": "./api/index.js"
		},
		"./route-handlers": {
			"types": "./route-handlers/index.d.ts",
			"import": "./route-handlers/index.js"
		},
		"./server-actions": {
			"types": "./server-actions/index.d.ts",
			"import": "./server-actions/index.js"
		},
		"./server-actions*": {
			"import": "./server-actions/*"
		}
	},
	"bin": {
		"nextdato": "./cli/index.js"
	},
	"engines": {
		"node": ">=0.18"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/is-integer": "^1.0.2",
		"commander": "^11.1.0",
		"cosmiconfig": "^9.0.0",
		"cosmiconfig-ts-loader": "^0.1.1",
		"cosmiconfig-typescript-loader": "^6.1.0",
		"datocms-listen": "^0.1.15",
		"dedent-js": "^1.0.1",
		"dotenv": "^16.4.4",
		"ico-utils": "^0.1.4",
		"is-integer": "^1.0.7",
		"object-traversal": "^1.0.1",
		"pluralize": "^8.0.0",
		"prettier": "^3.5.3",
		"pretty-bytes": "^6.1.1",
		"react-markdown": "^9.0.1",
		"remark-breaks": "^4.0.0",
		"remark-gfm": "^4.0.0",
		"table": "^6.8.1"
	},
