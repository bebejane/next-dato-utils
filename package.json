{
	"name": "next-dato-utils",
	"version": "2.0.45",
	"description": "",
	"type": "module",
	"main": "./index.js",
	"module": "./index.js",
	"scripts": {
		"build": "node ./clean.js && npm run tsc && npm run copy-files",
		"push": "npm run build && git add . && git commit -m \"update\" && git push",
		"release": "npm run push && npm version patch --force && git push && git push --tags && git tag -f latest && git push --tags && gh release create $(git describe --tags --abbrev=0) --generate-notes",
		"dev": "npx nodemon --watch \"src/**/*\" --watch \"./package.json\" -e tsx,ts,scss,json --exec \"npm run build\"",
		"dev:cli": "tsx src/cli/index.ts",
		"tsc": "tsc -p tsconfig.json",
		"copy-files": "copyfiles -u 1 src/**/*.scss ./"
	},
	"exports": {
		"./package.json": "./package.json",
		"./utils": {
			"types": "./utils/index.d.ts",
			"import": "./utils/index.js",
			"bun": "./utils/index.js"
		},
		"./config": {
			"types": "./config/index.d.ts",
			"import": "./config/index.js",
			"bun": "./config/index.js"
		},
		"./components": {
			"types": "./components/index.d.ts",
			"import": "./components/index.js",
			"bun": "./components/index.js"
		},
		"./hooks": {
			"types": "./hooks/index.d.ts",
			"import": "./hooks/index.js",
			"bun": "./hooks/index.js"
		},
		"./api": {
			"types": "./api/index.d.ts",
			"import": "./api/index.js",
			"bun": "./api/index.js"
		},
		"./route-handlers": {
			"types": "./route-handlers/index.d.ts",
			"import": "./route-handlers/index.js",
			"bun": "./route-handlers/index.js"
		},
		"./router": {
			"types": "./router/index.d.ts",
			"import": "./router/index.js",
			"bun": "./router/index.js"
		},
		"./server-actions": {
			"types": "./server-actions/index.d.ts",
			"import": "./server-actions/index.js",
			"bun": "./server-actions/index.js"
		},
		"./server-actions*": {
			"import": "./server-actions/*",
			"bun": "./server-actions/*"
		}
	},
	"bin": {
		"nextdato": "./cli/index.js"
	},
	"engines": {
		"node": ">=0.18"
	},
