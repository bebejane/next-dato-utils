{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./",
    "strict": true,
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "jsx": "react-jsx",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "skipLibCheck": true,
    "paths": {
      "@/datocms.config": [
        "./datocms.config.ts"
      ]
    }
  },
  "exclude": [],
  "include": [
    "./src",
    "./types"
  ]
}